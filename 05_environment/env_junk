#!/bin/bash
#
# print env

command="env CheckerIsBrokenCallGuillaumeAndJulien"

# clean up
stop_shell

# run command
echo "$command" | $SHELL > $OUTPUTFILE 2> $ERROROUTPUTFILE &
$command > $EXPECTED 2> $ERROREXPECTED

# wait a little bit
$SLEEP $SLEEPSECONDS

# check the result
check_diff

# error
print_error

# clean up
stop_shell
